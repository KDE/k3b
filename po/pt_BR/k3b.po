#, kde-format
msgid "Rock Ridge"
msgstr "Rock Ridge"

#: projects/k3bdataimagesettingswidget.cpp:214
#, kde-format
msgid "Joliet"
msgstr "Joliet"

#: projects/k3bdataimagesettingswidget.cpp:216
#, kde-format
msgid "UDF"
msgstr "UDF"

#: projects/k3bdataimagesettingswidget.cpp:218
#, kde-format
msgid "Custom (ISO 9660 only)"
msgstr "Personalizado (apenas ISO 9660)"

